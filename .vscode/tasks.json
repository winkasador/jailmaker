{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Run Jailbreak in Debug Mode",
      "type": "shell",
      "command": "dotnet",
      "args": [
        "run",
        "--project",
        "${workspaceFolder}/Jailbreak/Jailbreak.csproj",
        "--",
        "--debug"
      ],
      "options": {
        "cwd": "${workspaceFolder}/Jailbreak"
      },
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": false
      }
    }
  ]
}
